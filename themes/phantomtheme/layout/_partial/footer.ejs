<!-- Footer -->
<footer id="footer">
    <div class="inner">
        
        <section>
            <h2>Contact</h2>
            <ul class="icons">
                <% if (theme.twitter_url) { %>
                    <li><a href="<%- theme.twitter_url %>" class="icon style2 fa-twitter" target="_blank" ><span class="label">Twitter</span></a></li>
                <% } %>
                <% if (theme.linkedin_url) { %>
                    <li><a href="<%- theme.linkedin_url %>" class="icon style2 fa-linkedin" target="_blank" ><span class="label">LinkedIn</span></a></li>
                <% } %>
            
            </ul>
        </section>
        <ul class="copyright">
            <li>&copy; Leenke De Donder</li>
            <li>Theme: created by <a href="http://html5up.net" target="_blank">HTML5 UP</a></li>
